<%= form_with model: @user do |form|%>
  <%= form.label :username%>
  <%= form.text_field :username, value: 'example'%>

  <%= form.label :email%>
  <%= form.email_field :email, value: 'example@example.com'%>

  <%= form.label :password%>
  <%= form.password_field :password, value: '*****'%>

  <%= form.submit%>
<% end%>